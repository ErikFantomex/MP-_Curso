#!/bin/bash
#SBATCH --nodes=1                #numero de nodos (maximo 13)
#SBATCH --job-name=Horacio       #nombre del job
#SBATCH --ntasks-per-node=15
#SBATCH --time=1:00:00      #tiempo de ejecucion (maximo 168 hrs)
#SBATCH --partition=general      #cola de slurm
#SBATCH --constraint=broadwell

module load intel/oneAPI-2021

mpicc Primosv2.c -o parallel_primos_v2 -lm

echo "Contando primos en paralelo v2"
for i in $(seq 1 15)
do
    mpirun -np $i ./parallel_primos_v2 2000000
done
