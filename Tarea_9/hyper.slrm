#!/bin/bash
# PBS Job Script para uso de mpi

#Modifique segun el numero de procesadores necesitados
#PBS -l nodes=1:ppn=4
#PBS -l walltime=00:10:00
#PBS -N MPI
#PBS -V
#PBS -q general
#PBS -k eo


module load intel/oneAPI-2021

mpicc hyperquick.c -o hyperquick  -lm 

for t in $(seq 1 16)
do
  echo "  ------------------------ Esperando $t segundos ------------"
  mpirun -np 16  ./hyperquick datos.dat $t >> datosnumpy1.dat
done

